marks = []
for i in range(1, 6):
    mark = float(input(f"subject {i}: "))
    marks.append(mark)


average_marks = sum(marks) / len(marks)

if 90 <= average_marks <= 100:
    grade = 'A'
elif 80 <= average_marks < 90:
    grade = 'B'
elif 70 <= average_marks < 80:
    grade = 'C'
elif 60 <= average_marks < 70:
    grade = 'D'
else:
    grade = 'F'


print(f"Average Marks: {average_marks:.2f}")
print(f"Grade: {grade}")

